{
    "name": "Jake",
    "version": "1.0",
    "description": "Live code examples for API documentation",
    "content_scripts": [
        {
          "matches": [
            "<all_urls>"
          ],
          "js": ["background.js","jquery.min.js"]
        }
    ],
    "permissions": [
        "tabs",
        "activeTab",
        "<all_urls>"
    ],
    "browser_action": {
        "default_popup": "popup.html" 
    },
    "manifest_version": 2
}